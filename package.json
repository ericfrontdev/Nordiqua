{
  "name": "nordiqua",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "nordiqua/backend",
    "nordiqua/frontend"
  ],
  "scripts": {
    "dev:backend": "npm run dev --workspace=nordiqua/backend",
    "dev:frontend": "npm run dev --workspace=nordiqua/frontend",
    "dev": "npm-run-all --parallel dev:*",
    "test": "npm test --workspaces"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}